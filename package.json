{
  "name": "multi-layer-dss",
  "version": "0.1.1",
  "description": "A npm package that builds a multi-layered decision support system (DSS) for Node.js. You can program what each layer and the total flow does and returns, with a simple JS function.",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "build": "npm run lint && tsc && npm run test",
    "lint": "./node_modules/.bin/eslint lib/classes/*",
    "test": "mocha --reporter spec",
    "debug": "node ./debug.js",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha test/*.js -- -R spec",
    "cover": "./node_modules/istanbul/lib/cli cover _mocha test/*.js",
    "report-coverage": "cat ./coverage/lcov.info | codecov"
  },
  "keywords": [
    "drools",
    "dss",
    "multi-layer",
    "diy",
    "decision",
    "decision support system"
  ],
  "author": "Kevin Carrasco <kevin.carrasco@clupik.com>",
  "license": "MIT",
  "homepage": "https://github.com/Brisingeros/multi-layer-dss",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "1.5.0",
    "@typescript-eslint/parser": "1.5.0",
    "chai": "4.2.0",
    "codecov.io": "0.1.6",
    "eslint": "5.15.3",
    "istanbul": "0.4.5",
    "mocha": "6.0.2",
    "typescript": "3.3.4000"
  },
  "dependencies": {}
}
